<templateSet group="AndroidComments">
  <template name="ccode" value="&lt;code&gt;$code$&lt;/code&gt;" description="Code tag for javadoc" toReformat="false" toShortenFQNames="true">
    <variable name="code" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="cfalse" value="&lt;code&gt;false&lt;/code&gt;" description="Puts false in a comment section" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="clink" value="{@link $classToLink$}" description="Link from within javadoc" toReformat="false" toShortenFQNames="true">
    <variable name="classToLink" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="ctrue" value="&lt;code&gt;true&lt;/code&gt;" description="Puts true in a comment section" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="fixme" value="// FIXME: $date$ $todo$" description="Adds // FIXME" toReformat="true" toShortenFQNames="true">
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="todo" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="noop" value="/* no-op */" description="Indicates that a method does not have any operations" toReformat="true" toShortenFQNames="true">
    <context />
  </template>
  <template name="stopship" value="// STOPSHIP: $date$ $todo$ " description="Adds // STOPSHIP" toReformat="true" toShortenFQNames="true">
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="todo" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="todo" value="// TODO: $date$ $todo$ " description="Adds // TODO" toReformat="true" toShortenFQNames="true">
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="todo" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>